{
  "name": "projektor",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "parcel --dist-dir dev-dist src/index.html",
    "build": "yarn clean-dist && parcel build --dist-dir dist src/index.html",
    "build:dev": "yarn clean-dev-dist && NODE_ENV=development parcel build --dist-dir dev-dist src/index.html",
    "format": "prettier --write \"src/**/*.ts*\" \"src/**/*.spec.ts*\" \"cypress/fixtures/**/*.json\" \"cypress/**/*.js\"",
    "test": "jest",
    "test:projektor:ci": "yarn test || yarn projektor-publish --configFile=projektor.jest.json",
    "projektor:jest:ci": "yarn projektor-publish --configFile=projektor.jest.json",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:single": "cypress run -s cypress/integration/attachments.spec.js",
    "cy:test": "yarn clean-results && start-server-and-test start http://localhost:1234 cy:run",
    "cy:test:projektor:ci": "yarn cy:test || yarn projektor-publish --configFile=projektor.cypress.json",
    "projektor:cypress:no-attachments": "projektor-publish --configFile=projektor.cypress.no.attachments.json",
    "projektor:cypress": "env-cmd -f ~/.env projektor-publish --configFile=projektor.cypress.json",
    "projektor:jest": "env-cmd -f ~/.env projektor-publish --configFile=projektor.jest.json",
    "clean-results": "rimraf test-results/*.xml",
    "clean-dist": "rimraf dist",
    "clean-dev-dist": "rimraf dev-dist"
  },
  "devDependencies": {
    "@testing-library/cypress": "8.0.2",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.2",
    "@types/jest": "27.4.0",
    "axios-mock-adapter": "1.20.0",
    "chai-colors": "1.0.1",
    "css-mediaquery": "0.1.2",
    "cypress": "9.4.1",
    "del": "6.0.0",
    "env-cmd": "10.1.0",
    "jest": "27.4.7",
    "jest-junit": "13.0.0",
    "parcel": "2.2.1",
    "prettier": "2.5.1",
    "projektor-publish": "3.10.0",
    "resize-observer-polyfill": "1.5.1",
    "rimraf": "3.0.2",
    "start-server-and-test": "1.14.0",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5",
    "wait-for-expect": "3.0.2"
  },
  "dependencies": {
    "@material-table/core": "4.3.27",
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.60",
    "@reach/router": "1.3.4",
    "@types/node": "16.11.22",
    "@types/reach__router": "1.3.10",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/styled-components": "5.1.21",
    "axios": "0.25.0",
    "axios-cache-adapter": "2.7.3",
    "axios-case-converter": "0.9.0",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "moment-timezone": "0.5.34",
    "pretty-bytes": "5.6.0",
    "query-string": "7.1.0",
    "react": "17.0.2",
    "react-copy-to-clipboard": "5.0.4",
    "react-dom": "17.0.2",
    "react-horizontal-stacked-bar-chart": "8.15.2",
    "react-scroll": "1.8.4",
    "recharts": "2.1.8",
    "styled-components": "5.3.3",
    "use-query-params": "1.2.3"
  },
  "jest-junit": {
    "includeConsoleOutput": "true",
    "outputDirectory": "jestResults",
    "classNameTemplate": "{classname}",
    "usePathForSuiteName": "true"
  }
}

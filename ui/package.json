{
  "name": "projektor",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "parcel --dist-dir dev-dist src/index.html",
    "build": "parcel build --dist-dir dist src/index.html",
    "build:dev": "NODE_ENV=development parcel build --dist-dir dev-dist src/index.html",
    "format": "prettier --write \"src/**/*.ts*\" \"src/**/*.spec.ts*\" \"cypress/fixtures/**/*.json\" \"cypress/**/*.js\"",
    "test": "jest",
    "test:projektor:ci": "yarn test || yarn projektor-publish --configFile=projektor.jest.json",
    "projektor:jest:ci": "yarn projektor-publish --configFile=projektor.jest.json",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:single": "cypress run -s cypress/integration/attachments.spec.js",
    "cy:test": "yarn clean-results && start-server-and-test start http://localhost:1234 cy:run",
    "cy:test:projektor:ci": "yarn cy:test || yarn projektor-publish --configFile=projektor.cypress.json",
    "projektor:cypress:no-attachments": "projektor-publish --configFile=projektor.cypress.no.attachments.json",
    "projektor:cypress": "env-cmd -f ~/.env projektor-publish --configFile=projektor.cypress.json",
    "projektor:jest": "env-cmd -f ~/.env projektor-publish --configFile=projektor.jest.json",
    "clean-results": "rimraf test-results/*.xml"
  },
  "devDependencies": {
    "@testing-library/cypress": "7.0.7",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "11.2.7",
    "@types/jest": "26.0.23",
    "axios-mock-adapter": "1.17.0",
    "chai-colors": "1.0.1",
    "css-mediaquery": "0.1.2",
    "cypress": "6.7.1",
    "del": "6.0.0",
    "env-cmd": "10.1.0",
    "jest": "27.2.5",
    "jest-junit": "13.0.0",
    "parcel": "2.0.1",
    "prettier": "2.4.1",
    "projektor-publish": "3.7.0",
    "resize-observer-polyfill": "1.5.1",
    "rimraf": "3.0.2",
    "start-server-and-test": "1.10.11",
    "ts-jest": "27.0.3",
    "typescript": "4.3.4",
    "wait-for-expect": "3.0.2"
  },
  "dependencies": {
    "@material-ui/core": "4.9.14",
    "@material-ui/icons": "4.4.3",
    "@material-ui/lab": "4.0.0-alpha.53",
    "@reach/router": "1.3.4",
    "@types/node": "12.11.1",
    "@types/reach__router": "1.3.6",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "@types/styled-components": "5.1.0",
    "axios": "0.21.2",
    "axios-cache-adapter": "2.5.0",
    "axios-case-converter": "0.7.0",
    "lodash": "4.17.21",
    "material-table": "1.55.0",
    "moment": "2.24.0",
    "moment-timezone": "0.5.31",
    "pretty-bytes": "5.3.0",
    "query-string": "6.13.8",
    "react": "16.12.0",
    "react-copy-to-clipboard": "5.0.4",
    "react-dom": "16.12.0",
    "react-horizontal-stacked-bar-chart": "8.15.2",
    "react-scroll": "1.7.14",
    "recharts": "2.0.8",
    "styled-components": "5.1.0",
    "use-query-params": "1.1.9"
  },
  "jest-junit": {
    "includeConsoleOutput": "true",
    "outputDirectory": "jestResults",
    "classNameTemplate": "{classname}",
    "usePathForSuiteName": "true"
  }
}

{
  "name": "projektor",
  "version": "1.0.0",
  "main": "src/index.tsx",
  "license": "MIT",
  "scripts": {
    "start": "parcel --out-dir dev-dist src/index.html",
    "build": "parcel build src/index.html",
    "build:dev": "NODE_ENV=development parcel build --out-dir dev-dist src/index.html",
    "format": "prettier --write \"src/**/*.ts*\" \"src/**/*.spec.ts*\" \"cypress/fixtures/**/*.json\" \"cypress/**/*.js\"",
    "test": "jest",
    "test:projektor:ci": "yarn test || yarn projektor-publish --configFile=projektor.jest.json",
    "projektor:jest:ci": "yarn projektor-publish --configFile=projektor.jest.json",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:test": "yarn clean-results && start-server-and-test start http://localhost:1234 cy:run",
    "cy:test:projektor:ci": "yarn cy:test || yarn projektor-publish --configFile=projektor.cypress.json",
    "projektor:cypress": "env-cmd -f ~/.env projektor-publish --configFile=projektor.cypress.json",
    "projektor:jest": "env-cmd -f ~/.env projektor-publish --configFile=projektor.jest.json",
    "clean-results": "rimraf test-results/*.xml"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.5.0",
    "@testing-library/react": "10.0.3",
    "@types/jest": "25.1.4",
    "axios-mock-adapter": "1.17.0",
    "chai-colors": "1.0.1",
    "cypress": "4.11.0",
    "env-cmd": "10.1.0",
    "jest": "25.1.0",
    "jest-junit": "10.0.0",
    "parcel-bundler": "1.12.4",
    "parcel-plugin-clean-dist": "0.0.6",
    "prettier": "2.0.2",
    "projektor-publish": "2.6.0",
    "rimraf": "3.0.2",
    "start-server-and-test": "1.10.11",
    "ts-jest": "25.3.0",
    "typescript": "3.7.4"
  },
  "dependencies": {
    "@material-ui/core": "4.9.14",
    "@material-ui/icons": "4.4.3",
    "@material-ui/lab": "4.0.0-alpha.53",
    "@reach/router": "1.3.4",
    "@types/axios-case-converter": "0.3.1",
    "@types/node": "12.11.1",
    "@types/reach__router": "1.2.6",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "@types/styled-components": "5.1.0",
    "axios": "0.19.2",
    "axios-cache-adapter": "2.5.0",
    "axios-case-converter": "0.3.0",
    "material-table": "1.55.0",
    "moment": "2.24.0",
    "pretty-bytes": "5.3.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-horizontal-stacked-bar-chart": "8.15.2",
    "react-scroll": "1.7.14",
    "styled-components": "5.1.0",
    "use-query-params": "0.4.1"
  },
  "jest-junit": {
    "includeConsoleOutput": "true",
    "outputDirectory": "jestResults"
  }
}

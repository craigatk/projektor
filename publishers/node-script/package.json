{
  "name": "projektor-publish",
  "version": "2.3.1",
  "homepage": "https://projektor.dev/docs/node-script/",
  "repository": {
    "type": "git",
    "url": "https://github.com/craigatk/projektor.git"
  },
  "main": "./index.js",
  "bin": "./bin/projektorPublish.js",
  "author": "Craig Atkinson",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "jest",
    "format": "prettier --write src/*.js src/**/*.js",
    "projektor": "env-cmd -f ~/.env projektor-publish"
  },
  "dependencies": {
    "axios": "0.19.2",
    "glob": "7.1.4",
    "minimist": "1.2.5"
  },
  "devDependencies": {
    "axios-mock-adapter": "1.17.0",
    "env-cmd": "10.1.0",
    "jest": "25.1.0",
    "jest-junit": "10.0.0",
    "prettier": "2.0.1",
    "projektor-publish": "2.2.2",
    "wait-for-expect": "3.0.1"
  },
  "jest": {
    "reporters": [
      "default",
      "jest-junit"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/publish-functional-test/"
    ],
    "collectCoverage": true
  },
  "jest-junit": {
    "outputDirectory": "testResults",
    "includeConsoleOutput": "true"
  }
}

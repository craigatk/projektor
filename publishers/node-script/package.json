{
  "name": "projektor-publish",
  "version": "3.10.2",
  "homepage": "https://projektor.dev/docs/node-script/",
  "repository": {
    "type": "git",
    "url": "https://github.com/craigatk/projektor.git"
  },
  "main": "./index.js",
  "bin": "./bin/projektorPublish.js",
  "author": "Craig Atkinson",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "jest",
    "format": "prettier --write src/*.js src/**/*.js",
    "projektor": "env-cmd -f ~/.env projektor-publish"
  },
  "dependencies": {
    "axios": "1.1.3",
    "glob": "8.0.3",
    "lodash": "4.17.21",
    "minimist": "1.2.7",
    "node-gzip": "1.1.2"
  },
  "devDependencies": {
    "axios-mock-adapter": "1.21.2",
    "env-cmd": "10.1.0",
    "jest": "29.3.1",
    "jest-junit": "14.0.1",
    "prettier": "2.8.1",
    "projektor-publish": "3.10.2",
    "wait-for-expect": "3.0.2"
  },
  "jest": {
    "reporters": [
      "default",
      "jest-junit"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/publish-functional-test/",
      "/util/"
    ],
    "collectCoverage": true
  },
  "jest-junit": {
    "outputDirectory": "testResults",
    "includeConsoleOutput": "true",
    "classNameTemplate": "{classname}",
    "usePathForSuiteName": "true"
  }
}
